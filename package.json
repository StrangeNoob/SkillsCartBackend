{
  "name": "skillscart",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node build/src/index.js",
    "tsc-watch": "tsc -w",
    "start:dev": "nodemon",
    "watch": "nodemon --watch build/src build/src/index.js",
    "clean": "rm -rf build && mkdir build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "StrangeNoob",
  "license": "ISC",
  "dependencies": {
    "@hapi/hapi": "^20.1.5",
    "@hapi/inert": "^6.0.3",
    "@hapi/vision": "^6.1.0",
    "bluebird": "^3.7.2",
    "dotenv": "^10.0.0",
    "hapi-swagger": "^14.2.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mongodb": "^3.6.9",
    "mongoose": "^5.13.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.35",
    "@types/confidence": "^1.4.30",
    "@types/hapi__hapi": "^20.0.8",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.6.1",
    "@types/node-fetch": "^2.5.10",
    "node": "^16.4.0",
    "nodemon": "^2.0.9",
    "ts-node": "^10.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  },
  "_moduleAliases": {
    "@root": "build/src",
    "@models": "build/src/models",
    "@utils": "build/src/utils",
    "@config": "build/src/config"
  }
}
